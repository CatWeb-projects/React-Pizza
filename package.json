{
  "name": "initial-start-projects",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.7",
    "eslint-config-airbnb": "^19.0.4",
    "estafette": "^1.0.18",
    "estafette-router": "^2.2.1",
    "json-server": "^0.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-inlinesvg": "^4.1.3",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "sass": "^1.71.1",
    "typescript": "^5.4.2",
    "vscode-eslint-debug": "^1.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "server": "json-server --watch db.json localhost -p 3005",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lerna:lint:js": "lerna run lint:js:fix",
    "format": "npx prettier --write \"src/**/*.{js,jsx,ts,tsx,json,scss}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint -c .eslintrc --fix --color"
    ],
    "src/**/*.{js,jsx,scss}": [
      "npx prettier --write -c .prettierrc"
    ]
  }
}
